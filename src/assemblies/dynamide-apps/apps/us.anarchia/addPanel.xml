<?xml version="1.0"?>
<!DOCTYPE dynamideForm>
<dynamideForm>
  <event language="beanshell" name="addPanel_onAction"><![CDATA[//no escapes
    System.out.println("\n\n\n\n=============>>>>>>>>>>>>>>>>>>>> addPanel in addPanel_onAction "+event.action);
  ]]></event>
  <widgets>
    <widget id="session1">
      <properties>
        <property name="type">
          <value>com.dynamide.session</value>
        </property>
      </properties>
    </widget>
    <widget id="hiddenPageID">
      <properties>
        <property name="field">
          <value>page</value>
        </property>
        <property name="type">
          <value>com.dynamide.hidden</value>
        </property>
        <property name="value">
          <datatype>com.dynamide.datatypes.WebmacroDatatype</datatype>
          <value>$pageID</value>
        </property>
      </properties>
    </widget>
    <widget id="imgURL">
      <properties>
        <property name="caption">
          <value>image url</value>
        </property>
        <property name="captionClass">
          <value>caption</value>
        </property>
        <property name="captionWidth">
          <value>180</value>
        </property>
        <property name="name">
          <value>imgURL</value>
        </property>
        <property name="type">
          <value>com.dynamide.edit</value>
        </property>
      </properties>
    </widget>
    <widget id="panelCaption">
      <properties>
        <property name="caption">
          <value>Caption</value>
        </property>
        <property name="captionClass">
          <value>caption</value>
        </property>
        <property name="captionWidth">
          <value>180</value>
        </property>
        <property name="name">
          <value>panelCaption</value>
        </property>
        <property name="type">
          <value>com.dynamide.edit</value>
        </property>
      </properties>
    </widget>
    <widget id="userID">
      <properties>
        <property name="caption">
          <value>User ID</value>
        </property>
        <property name="captionClass">
          <value>caption</value>
        </property>
        <property name="captionWidth">
          <value>180</value>
        </property>
        <property name="name">
          <value>userID</value>
        </property>
        <property name="type">
          <value>com.dynamide.edit</value>
        </property>
      </properties>
    </widget>
    <widget id="submit1">
      <properties>
        <property name="action">
          <value>doSubmitThing</value>
        </property>
        <property name="caption">
          <value>Submit</value>
        </property>
        <property name="type">
          <value>com.dynamide.submit</value>
        </property>
      </properties>
    </widget>
  </widgets>
  <htmlsrc browser="*"><![CDATA[
<HTML>
  <HEAD>
    <TITLE>$page.title</TITLE>
  </HEAD>
  <BODY id="page1" bottomMargin="15" leftMargin="10" topMargin="15" rightMargin="10" name="addPanel">
    <P>Add Panel</P>
    <FORM name="form$pageID" accept-charset="UNKNOWN" action="$session.applicationURI" method="get" encType="application/x-www-form-urlencoded">
      <SPAN class="widget" id="hiddenPageID"/>
      <SPAN class="widget" id="userID"/>
      <SPAN class="widget" id="imgURL"/>
      <SPAN class="widget" id="panelCaption"/>
      <SPAN class="widget" id="submit1"/>
    </FORM>
  </BODY>
</HTML>
]]></htmlsrc>
  <properties>
    <property name="formAction">
      <value>mojoAction</value>
    </property>
    <property name="formName">
      <value></value>
    </property>
    <property name="name">
      <readOnly>true</readOnly>
      <value>addPanel</value>
    </property>
    <property name="title">
      <value>Add Panel</value>
    </property>
    <property name="type">
      <readOnly>true</readOnly>
      <value>pagetypes/com.dynamide.page</value>
    </property>
  </properties>
</dynamideForm>