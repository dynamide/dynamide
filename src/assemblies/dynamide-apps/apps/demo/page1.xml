  <event language="beanshell" name="submit1_onClick"><![CDATA[//no escapes
    session.logDebug("page1 in submit1_onClick "+event.action);
  ]]></event>
  <event language="beanshell" name="page1_onAction"><![CDATA[//no escapes
  import com.dynamide.*;
    session.logDebug("page1 in onAction "+event.action);
    try {
        ji = new tcl.lang.Interp();

        String source = event.currentPage.getPropertyStringValue("tclsource");
        ji.eval("package require java");
        ji.eval("set event [java::new com.dynamide.event.ScriptEvent]");
        ji.eval("puts $event");
        tcl.lang.TclObject tclobj = ji.getResult();
        event.println("tcl obj to string:"+tclobj.toString());
    } catch (Throwable tcle){
        event.println("Tcl exception: "+tcle);
    }
    //event.println(event.toString());

    //Example Doco: %%  These all work:
    //    //This gets you a string value:
    //    System.out.println("page1 EVAL: "+event.session.expand("$session.page1.com_dynamide_img_logo.src"));
    //    //This gets you a string value:
    //    System.out.println("page1 EVAL: "+event.session.expand("$session.page1.qty1.value"));
    //    //This gets you a chain of real Java objects:
    //    System.out.println("page1 EVAL: "+event.session.get("page1").get("qty1").getValue());
    //    System.out.println("page1 EVAL: "+event.session.get("page1").get("qty1").render());

    String language = "";
    languageField = event.session.findField("language");
    if (languageField != null){
        language = languageField.getValue().toString();
    }
    String oldLanguage = event.session.getCurrentLanguageCode();
    if ( language.length() > 0 && (! oldLanguage.equals(language)) ){
        event.println("page1 changing language: "+language);
        event.session.loadInternationalization(language);
    } else {
        event.println("page1 not changing language. old: '"+oldLanguage+"' new: '"+language+"'");
    }

    makeAnErrorField = event.session.findField("makeAnError");
    customerField = event.session.findField("customer");
    boolean makeError = false;
    if (makeAnErrorField != null){
        makeError = (makeAnErrorField.getValue().equals("on"));
    }

    if (customerField != null){
        if (makeError){
            customerField.setErrorMessage("Error in field 'customer'");
        } else {
            customerField.setError(false);
        }
        event.println("Field name: "+customerField.get("name") + " Field value: "+customerField.getValue());
        //event.println(customerField.toString());
        IValueBean mb = event.session.getValueBean("myValueBean");
        if (mb != null){
            mb.setCellValue(1,1,customerField.getValue());
        }
    } else {
        event.println("Field was null");
    }
    totalField = event.session.findField("total");
    if (totalField != null && makeAnErrorField != null){
        event.println("Field name: "+makeAnErrorField.get("name") + " Field value: "+makeAnErrorField.getValue());
        if (makeError){
            totalField.set("value", "inEvent");
            totalField.setErrorMessage("Error in field 'total'");
        } else {
            totalField.setErrorMessage("");
            totalField.setError(false);
        }
    } else {
        event.println("Field 'total' was null");
    }
  ]]></event>
